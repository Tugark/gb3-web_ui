{
  "name": "gb3-poc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local-gb2": "ng serve --configuration=development-local-gb2",
    "build-local": "ng build --configuration=development-local-gb2",
    "build-local-gb2": "ng build --configuration=development-local-gb2",
    "build-dev-ebp": "ng build --configuration=production-dev-ebp",
    "build-uat": "ng build --configuration=production-uat",
    "build-staging": "ng build --configuration=production-staging",
    "build-production": "ng build --configuration=production",
    "watch": "ng build --configuration development",
    "test": "ng test",
    "test-ci": "ng test --no-watch --no-progress",
    "install-husky": "husky install",
    "generate-gb3-api": "npx swagger-typescript-api -p https://maps.zh.ch/api-docs/v3/swagger.yaml -o ./src/app/shared/models -n gb3-api-generated.interfaces.ts --no-client --extract-response-body --extract-request-body"
  },
