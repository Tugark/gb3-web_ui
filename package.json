    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@arcgis/core": "~4.27.0",
    "@ngrx/effects": "^16.0.1",
    "@ngrx/entity": "^16.0.1",
    "@ngrx/store": "^16.0.1",
    "@ngrx/store-devtools": "^16.0.1",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^18.13.0",
    "@types/twitter-for-web": "^0.0.2",
    "angular-oauth2-oidc": "^15.0.1",
    "angular-resizable-element": "^7.0.2",
    "dayjs": "^1.11.7",
    "immer": "^10.0.1",
    "material-icons": "^1.13.3",
    "ngx-ui-tour-md-menu": "^11.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13"
